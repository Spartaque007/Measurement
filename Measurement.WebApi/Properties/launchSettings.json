{
  "$schema": "https://json.schemastore.org/launchsettings.json",
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:50028",
      "sslPort": 44375
    }
  },
  "profiles": {
    "Measurement.WebApi": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "Measurements",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "applicationUrl": "https://localhost:7227;http://localhost:5227",
      "dotnetRunMessages": true
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/Measurements",
      "publishAllPorts": true,
      "useSSL": true
    }
  }
}